
<view class="about-warp p_20">
  <view class="logo-warp p_20 flex_cb">
    <view class="name">{{name}}</view>
    <image src="{{logo}}" class="logo" />
  </view>
  <view class="nav-warp">
    <view class="nav-item bor_b pr">
      <form report-submit='true' class='formBtn' bindsubmit='toCall'>
        <button class='formBtn' form-type='submit'></button>
      </form>
      <view class="left">
        <image class="icon" src="../../images/call.png" />
        <text>{{phone}}</text>
      </view>
      <image class="arrow" src="../../images/last_Dawn.png"></image>
    </view>
    <view class="nav-item bor_b pr">
      <view class="left">
        <button open-type="contact" class="formBtn"></button>
        <image class="icon" src="../../images/wechat.png" />
        <text>联系客服</text>
      </view>
      <image class="arrow" src="../../images/last_Dawn.png"></image>
    </view>
    <view class="nav-item bor_b pr">
      <form report-submit='true' class='formBtn' bindsubmit='toHelp'>
        <button class='formBtn' form-type='submit'></button>
      </form>
      <view class="left">
        <image class="icon" src="../../images/bang.png" />
        <text>帮助说明</text>
      </view>
      <image class="arrow" src="../../images/last_Dawn.png"></image>
    </view>
    <view class="nav-item bor_b" bindtap='toPosition'>
      <view class="left">
        <image class="icon" src="../../images/position.png" />
        <text>{{address}}</text>
      </view>
      <image class="arrow" src="../../images/last_Dawn.png"></image>
    </view>
  </view>
  <image src='{{qrcode}}' class='qrcode' mode='widthFix'></image>
  <view class='flex_af f_28 p_40'>版权归属 @宁德市东洋水产</view>
</view>